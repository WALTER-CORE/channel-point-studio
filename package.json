{
  "name": "channel-point-studio",
  "version": "1.0.0",
  "description": "Twitch Channel Points Hackathon 2021",
  "main": "index.js",
  "jest": {
    "verbose": true, 
    "roots": ["src/test"]
  },
  "scripts": {
    "client": "yarn --cwd client run start",
    "start": "node src/index",
    "server": "nodemon src/index",
    "debug": "DEBUG=express:* nodemon src/index",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "docker-up": "docker-compose -f docker-compose.yml down",
    "docker-down": "docker-compose -f docker-compose.yml up",
    "test": "jest"
  },
  "repository": "git+https://github.com/WALTER-CORE/channel-point-studio.git",
  "author": "Brian Jhong, James Jhong",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/WALTER-CORE/channel-point-studio/issues"
  },
  "homepage": "https://github.com/WALTER-CORE/channel-point-studio#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "mysql": "^2.18.1",
    "node": "^15.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
}
